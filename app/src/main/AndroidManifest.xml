<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yt.s_server">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ybbs"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ybbs"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".course.HomeworkActivity"></activity>
        <activity android:name=".home.LeaveCreateActivity" />
        <activity
            android:name=".user.LoginActivity"
            android:label="登录" />
        <activity
            android:name=".course.EolCourseMenuActivity"
            android:label="课程通知" />
        <activity
            android:name=".home.ExamActivity"
            android:label="考试信息" />
        <activity
            android:name=".home.ScoreActivity"
            android:label="我的成绩" />
        <activity
            android:name=".home.WorkActivity"
            android:label="待办事项" />
        <activity
            android:name=".home.AttendanceActivity"
            android:label="我的考勤" />
        <activity
            android:name=".home.LeaveActivity"
            android:label="我的请假" />
        <activity
            android:name=".home.ScheduleActivity"
            android:label="我的课表" />
        <activity
            android:name=".home.BorrowedListActivity"
            android:label="待还书籍" />
        <activity
            android:name=".home.HistoryListActivity"
            android:label="借阅历史" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>